{
  "name": "@sangdth/pw",
  "description": "Simple password manager",
  "version": "1.0.0-beta-40",
  "author": "Sang Dang @sangdth",
  "license": "MIT",
  "repository": "sangdth/pw",
  "homepage": "https://github.com/sangdth/pw",
  "bugs": "https://github.com/sangdth/pw/issues",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "postpack": "rm -f .oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "pack": "oclif-dev pack",
    "prepare": "tsc -p .",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/.oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pw",
    "plugins": [
      "@oclif/plugin-update",
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "bin": {
    "pw": "./bin/run"
  },
  "keywords": [
    "oclif"
  ],
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/plugin-update": "^1.3.9",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/cli-table": "^0.3.0",
    "@types/fs-extra": "^5.0.5",
    "@types/lowdb": "^1.0.9",
    "@types/usb": "^1.5.1",
    "chalk": "^2.4.1",
    "cli-table": "^0.3.1",
    "cli-ux": "^5.3.0",
    "clipboardy": "^2.0.0",
    "fs-extra": "^7.0.1",
    "fuse.js": "^3.4.6",
    "inquirer": "^6.3.1",
    "keytar": "^4.11.0",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "qrcode": "^1.4.4",
    "randomatic": "^3.1.1",
    "tslib": "^1.9.3",
    "usb": "^1.6.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.7",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.6",
    "@types/qrcode": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-oclif": "^0.1.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.10.0",
    "typescript": "^3.7.5"
  }
}
