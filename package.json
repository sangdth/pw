{
  "name": "@sangdth/pw",
  "description": "Simple password manager",
  "version": "1.0.0-beta-07",
  "author": "Sang Dang @sangdth",
  "bin": {
    "pw": "./bin/run"
  },
  "bugs": "https://github.com/sangdth/pw/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "@types/cli-table": "^0.3.0",
    "@types/inquirer": "0.0.41",
    "chalk": "^2.4.1",
    "cli-table": "^0.3.1",
    "cli-ux": "^5.2.1",
    "clipboardy": "^2.0.0",
    "inquirer": "^6.3.1",
    "randomatic": "^3.1.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.6",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.10.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sangdth/pw",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pw",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sangdth/pw",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
